// console.log(cycleOfAB(-4, 13))

const cycleOfAB = (a, b) => {
    let cycle = []
    for (let i = a; i <= b; i++) {
        cycle[i] = i
        lengthCycle = cycle.length
    }
    return { length: lengthCycle, value: cycle }
}

